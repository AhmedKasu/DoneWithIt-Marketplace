  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd ../client && npm run build",
    "build:server": "tsc",
    "lint": "concurrently 'npm run lint:server' 'npm run lint:client'",
    "lint:server": "eslint --ext .ts .",
    "lint:client": "cd ../client && npm run lint",
    "dev": "ts-node-dev --files src/index.ts",
    "start": "NODE_ENV=production node build/index.js",
    "migration:up": "node src/utils/migrate.js up",
    "migration:down": "node src/utils/migrate.js down",
    "seed:up": "node src/utils/seed.js up",
    "seed:down": "node src/utils/seed.js down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
