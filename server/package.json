    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.7",
    "@types/sequelize": "^4.28.17",
    "@types/umzug": "^2.3.5",
    "@types/validator": "^13.11.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "concurrently": "^8.2.2",
